{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/old-path",
      "destination": "/new-path"
    },
    {
      "source": "/(.*)",
      "destination": "index.js"
    }
  ]
}
